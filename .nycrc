{
  "watermarks": {
    "lines": [80, 100],
    "functions": [80, 100],
    "branches": [80, 100],
    "statements": [80, 100]
  },
  "cache": false,
  "check-coverage": false,
  "extension": [
    ".ts"
  ],
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "coverage/**",
    "node_modules/**",
    "tools/**",
    "test/**/*.ts",
    "**/*.d.ts",
    "**/*.[Tt]est.ts",
    "**/*.[Ss]pec.ts"
  ],
  "sourceMap": true,
  "reporter": [
    "text",
    "text-summary",
    "lcov"
  ],
  "all": true,
  "instrument": true
}
