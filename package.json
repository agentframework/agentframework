{
  "name": "agentframework",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=6.0.0"
  },
  "description": "TypeScript that scales",
  "main": "src/lib/index.ts",
  "scripts": {
    "cov": "node -r ts-node/register node_modules/.bin/istanbul cover --config=tools/istanbul.yaml node_modules/.bin/jasmine JASMINE_CONFIG_PATH=tools/jasmine.json",
    "build": "npm run lint && npm run test",
    "test": "node -r ts-node/register node_modules/.bin/jasmine JASMINE_CONFIG_PATH=tools/jasmine.json",
    "lint": "node_modules/.bin/standard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agentframework/agentframework.git"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true
  },
  "keywords": [
    "agent",
    "aop",
    "typescript",
    "es6",
    "proxy",
    "interceptor",
    "database"
  ],
  "author": "Ling Zhang",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/agentframework/agentframework/issues"
  },
  "homepage": "https://github.com/agentframework/agentframework#readme",
  "devDependencies": {
    "@types/jasmine": "^2.8.6",
    "istanbul": "1.1.0-alpha.1",
    "jasmine": "^3.3.1",
    "jasmine-spec-reporter": "^4.2.1",
    "ts-node": "^7.0.1",
    "typescript": "^3.2.4",
    "typescript-standard": "^0.3.36"
  }
}
